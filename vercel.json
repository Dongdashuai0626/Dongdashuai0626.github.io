{
  "version": 2,
  "builds": [
    {
      "src": "**/*.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "^/$",
      "dest": "/index.html"
    },
    {
      "src": "^/blog$",
      "dest": "/pages/blog.html"
    },
    {
      "src": "^/hobbies$",
      "dest": "/pages/hobbies.html"
    },
    {
      "src": "^/photography$",
      "dest": "/pages/photography.html"
    },
    {
      "src": "^/photography-fuzhou$",
      "dest": "/pages/photography-fuzhou.html"
    },
    {
      "src": "^/sitemap$",
      "dest": "/pages/sitemap.html"
    },
    {
      "src": "^/blog/3dgs$",
      "dest": "/pages/blog/blog-3dgs.html"
    },
    {
      "src": "^/blog/3dgs-deploy$",
      "dest": "/pages/blog/blog-3dgs-deploy.html"
    },
    {
      "src": "^/blog/dgs$",
      "dest": "/pages/blog/blog-dgs.html"
    },
    {
      "src": "^/blog/dsg-world$",
      "dest": "/pages/blog/blog-dsg-world.html"
    },
    {
      "src": "^/blog/gauss-tr$",
      "dest": "/pages/blog/blog-gauss-tr.html"
    },
    {
      "src": "^/blog/gaussian-cut$",
      "dest": "/pages/blog/blog-gaussian-cut.html"
    },
    {
      "src": "^/blog/gaussian-occ$",
      "dest": "/pages/blog/blog-gaussian-occ.html"
    },
    {
      "src": "^/blog/mvsplat$",
      "dest": "/pages/blog/blog-mvsplat.html"
    },
    {
      "src": "^/blog/self-splat$",
      "dest": "/pages/blog/blog-self-splat.html"
    },
    {
      "src": "^/blog/semantic-splat$",
      "dest": "/pages/blog/blog-semantic-splat.html"
    },
    {
      "src": "^/blog/splatformer$",
      "dest": "/pages/blog/blog-splatformer.html"
    },
    {
      "src": "^/(.+)\\.html$",
      "status": 301,
      "headers": {
        "Location": "/$1"
      }
    },
    {
      "src": "^/(.*)$",
      "dest": "/$1.html",
      "continue": true
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        }
      ]
    },
    {
      "source": "/assets/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}
